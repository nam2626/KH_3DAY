<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kh.mapper.CarMapper">
  <select id="selectAllCar" resultType="com.kh.dto.CarDTO">
  	select * from car
  </select>
  <delete id="deleteCar">
 	delete from car where id like #{id}
  </delete>
</mapper>